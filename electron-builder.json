{
  "appId": "com.template.app",
  "productName": "template",
  "asar": true,
  "extends": null,
  "directories": {
    "buildResources": "assets"
  },
  "win": {
    "target": "nsis",
    "artifactName": "${productName}-${version}.${ext}"
  },
  "nsis": {
    "oneClick": true,
    "uninstallDisplayName": "${productName}"
  },
  "files": [
    "./build/**/*",
    "./scripts/electron-main.js"
  ]
}